{
  "manifest_version": 2,
  "name": "Hide uuid",
  "version": "0.0.1",
  "description": "Find uuid and hide in pages of specified URL",
  "page_action": {
    "default_icon": {
      "16": "images/16x16_disabled.png",
      "24": "images/24x24_disabled.png",
      "32": "images/32x32_disabled.png"
    }
  },
  "background": {
    "persistent": false,
    "scripts": [
      "core/core.js",
      "background.js"
    ]
  },
  "icons": {
    "16": "images/16x16.png",
    "48": "images/48x48.png",
    "128": "images/128x128.png"
  },
  "options_ui": {
    "page": "options/options.html"
  },
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "https://*/*"
  ]
}